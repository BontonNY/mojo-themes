{
  "components/account/edit-account": "<div class=\"col-12 col-lg-5 mb-5 py-3\">\n    <form action=\"{{urls.account.update_action}}\" data-edit-account-form class=\"form\" method=\"post\">\n        {{inject 'firstName' (lang 'forms.validate.account.edit.first_name')}}\n        {{inject 'lastName' (lang 'forms.validate.account.edit.last_name')}}\n        {{inject 'phoneNumber' (lang 'forms.validate.account.edit.phone')}}\n        {{inject 'currentPassword' (lang 'forms.validate.account.edit.password')}}\n        <fieldset class=\"\">\n            <div class=\"\">\n                <div class=\"form-group\">\n                    <label class=\"form-label\" for=\"account_firstname\">\n                        {{lang 'account.settings.first_name' }}\n                        <span class=\"required\">{{lang 'common.required' }}</span>\n                    </label>\n                    <input type=\"text\" class=\"form-input form-control\" name=\"account_firstname\" id=\"account_firstname\" value=\"{{forms.edit_account.first_name}}\">\n                </div>\n                <div class=\"form-group\">\n                    <label class=\"form-label\" for=\"account_lastname\">\n                        {{lang 'account.settings.last_name' }}\n                        <span class=\"required\">{{lang 'common.required' }}</span>\n                    </label>\n                    <input type=\"text\" class=\"form-input form-control\" name=\"account_lastname\" id=\"account_lastname\" value=\"{{forms.edit_account.last_name}}\">\n                </div>\n                <div class=\"form-group\">\n                    <label class=\"form-label\" for=\"account_companyname\">\n                        {{lang 'account.settings.company' }}\n                    </label>\n                    <input type=\"text\" class=\"form-input form-control\" name=\"account_companyname\" id=\"account_companyname\" value=\"{{forms.edit_account.company_name}}\">\n                </div>\n                <div class=\"form-group\">\n                    <label class=\"form-label\" for=\"account_phone\">\n                        {{lang 'account.settings.phone' }}\n                    </label>\n                    <input type=\"text\" class=\"form-input form-control\" name=\"account_phone\" id=\"account_phone\" value=\"{{forms.edit_account.phone}}\">\n                </div>\n                {{#each forms.edit_account.fields}}\n                {{{dynamicComponent 'components/common/forms'}}}\n                {{/each}}\n            </div>\n        </fieldset>\n        <div class=\"form-actions\">\n            <button class=\"btn btn-primary\">{{lang 'account.settings.update_details'}}</button>\n        </div>\n    </form>\n</div>\n",
  "components/common/forms/date": "<div class=\"form-group\" id=\"{{id}}\" data-validation=\"{{validation}}\" {{#if private_id}}data-type=\"{{private_id}}\"{{/if}}>\r\n    <label class=\"form-label\" for=\"{{id}}\" >{{label}}\r\n    {{#if required}}<span class=\"required\">{{lang 'common.required' }}</span>{{/if}}</label>\r\n    <div class=\"form-row form-row--third\">\r\n        <div class=\"form-field\">\r\n            <select class=\"form-control\" name=\"{{month.name}}\" id=\"{{id}}_month\" data-label=\"month\"  aria-required=\"{{required}}\" {{#if private_id}}data-field-type=\"{{private_id}}\"{{/if}}>\r\n                {{#each month.items}}\r\n                    <option {{#if ../month.value '==' value}} selected {{/if}}value=\"{{value}}\">{{label}}</option>\r\n                {{/each}}\r\n            </select>\r\n        </div>\r\n        <div class=\"form-field\">\r\n            <select class=\"form-control\" name=\"{{day.name}}\" id=\"{{id}}_day\" data-label=\"day\" aria-required=\"{{required}}\" {{#if private_id}}data-field-type=\"{{private_id}}\"{{/if}}>\r\n                {{#each day.items}}\r\n                    <option {{#if ../day.value '==' value}} selected {{/if}}value=\"{{value}}\">{{label}}</option>\r\n                {{/each}}\r\n            </select>\r\n        </div>\r\n        <div class=\"form-field\">\r\n            <select class=\"form-control\" name=\"{{year.name}}\" id=\"{{id}}_year\" data-label=\"year\" aria-required=\"{{required}}\" {{#if private_id}}data-field-type=\"{{private_id}}\"{{/if}}>\r\n                {{#each year.items}}\r\n                    <option {{#if ../year.value '==' value}} selected {{/if}}value=\"{{value}}\">{{label}}</option>\r\n                {{/each}}\r\n            </select>\r\n        </div>\r\n    </div>\r\n</div>\r\n",
  "components/common/forms/multiline": "<div class=\"form-group\" id=\"{{id}}\" data-validation=\"{{validation}}\">\r\n    <label class=\"form-label\" for=\"{{id}}_input\">{{label}}\r\n        {{#if required}}\r\n            <span class=\"required\">{{lang 'common.required' }}</span>\r\n        {{/if}}\r\n    </label>\r\n    <textarea name=\"{{name}}\" id=\"{{id}}_input\" data-label=\"{{label}}\" rows=\"{{rows}}\" aria-required=\"{{required}}\" class=\"form-control\" {{#if private_id}}data-field-type=\"{{private_id}}\"{{/if}}>{{value}}</textarea>\r\n</div>\r\n",
  "components/common/forms/number": "<div class=\"form-group\" id=\"{{id}}\" data-validation=\"{{validation}}\">\r\n    <label class=\"form-label\" for=\"{{id}}_input\">{{label}}\r\n        {{#if required}}<span class=\"required\">{{lang 'common.required' }}</span>{{/if}}\r\n    </label>\r\n    <input type=\"number\" class=\"form-control\" data-label=\"{{label}}\" data-rule=\"lowerThan\" min=\"{{limitfrom}}\" max=\"{{limitto}}\" title=\"{{lang 'forms.range' limitFrom=limitfrom limitTo=limitto}}\" value=\"{{value}}\" id=\"{{id}}_input\" name=\"{{name}}\" aria-required=\"{{required}}\" {{#if private_id}}data-field-type=\"{{private_id}}\"{{/if}}>\r\n</div>\r\n",
  "components/common/forms/password": "<div class=\"form-group\" id=\"{{id}}\" data-validation=\"{{validation}}\" {{#if private_id}}data-type=\"{{private_id}}\"{{/if}}>\r\n    <label class=\"form-label\" for=\"{{id}}_input\">{{label}}\r\n        {{#if required}}\r\n            <span class=\"required\">{{lang 'common.required' }}</span>\r\n        {{/if}}\r\n    </label>\r\n    <input type=\"password\"  class=\"form-control\" id=\"{{id}}_input\" data-label=\"{{label}}\" name=\"{{name}}\" value=\"{{value}}\" autocomplete=\"off\" aria-required=\"{{required}}\" {{#if private_id}}data-field-type=\"{{private_id}}\"{{/if}}>\r\n</div>\r\n",
  "components/common/forms/radio": "<div class=\"form-group\" id=\"{{id}}\" data-validation=\"{{validation}}\">\r\n    <label class=\"form-label\">{{label}}\r\n        {{#if required}}\r\n            <span class=\"required\">{{lang 'common.required' }}</span>\r\n        {{/if}}\r\n    </label>\r\n    {{#if radios}}\r\n        {{#each radios}}\r\n            <input \r\n                type=\"radio\"\r\n                class=\"form-radio\"\r\n                name=\"{{../name}}\"\r\n                id=\"{{../name}}_{{@index}}\"\r\n                value=\"{{value}}\"\r\n                data-label=\"{{label}}\"\r\n                aria-required=\"{{required}}\"\r\n                {{#if checked}}\r\n                    checked\r\n                {{/if}}\r\n                {{#if private_id}}\r\n                    data-field-type=\"{{private_id}}\"\r\n                {{/if}}>\r\n            <label class=\"form-label\" for=\"{{../name}}_{{@index}}\">{{label}}</label>\r\n        {{/each}}\r\n    {{/if}}\r\n</div>\r\n",
  "components/common/forms/selectortext": "<div class=\"form-group {{class_name}}\" id=\"{{id}}\" data-validation=\"{{validation}}\" {{#if private_id}}data-type=\"{{private_id}}\"{{/if}}>\r\n    <noscript>\r\n        <label class=\"form-label\" for=\"{{id}}_input\">{{label}}\r\n            {{#if required}}<span class=\"required\">{{lang 'common.required' }}</span>{{/if}}\r\n        </label>\r\n        <input type=\"text\" id=\"{{id}}_input\" name=\"{{name}}\" value=\"{{value}}\" class=\"form-control\" aria-required=\"{{required}}\" data-label=\"{{label}}\" />\r\n    </noscript>\r\n\r\n    <label class=\"form-label\" for=\"{{id}}_select\">{{label}}\r\n        {{#if required}}\r\n            <span class=\"required\">{{lang 'common.required' }}</span>\r\n        {{/if}}\r\n    </label>\r\n    <select name=\"{{name}}\" class=\"form-control\" aria-required=\"{{required}}\" id=\"{{id}}_select\" data-label=\"{{label}}\" {{#if private_id}}data-field-type=\"{{private_id}}\"{{/if}}>\r\n        {{#if options}}\r\n            <option value=\"\">{{chooseprefix}}</option>\r\n            {{#each options}}\r\n                <option {{#if selected}} selected {{/if}}value=\"{{value}}\">{{label}}</option>\r\n            {{/each}}\r\n        {{/if}}\r\n    </select>\r\n</div>\r\n",
  "components/common/forms/select": "<div class=\"form-group {{class_name}}\" id=\"{{id}}\" data-validation=\"{{validation}}\" {{#if private_id}} data-type=\"{{private_id}}\"\r\n    {{/if}}>\r\n    <label class=\"form-label\" for=\"{{id}}_select\">{{label}}\r\n        {{#if required}}<span class=\"required\">{{lang 'common.required' }}</span>{{/if}}\r\n    </label>\r\n    <select class=\"form-control {{class_name}}\" id=\"{{id}}_select\" data-label=\"{{label}}\" aria-required=\"{{required}}\" name=\"{{name}}\"\r\n        {{#if private_id}} data-field-type=\"{{private_id}}\" {{/if}}>\r\n        {{#if options}}\r\n        <option value=\"\">{{chooseprefix}}</option>\r\n        {{#each options}}\r\n        <option {{#if selected}} selected {{/if}} value=\"{{value}}\">{{{label}}}</option>\r\n        {{/each}}\r\n        {{/if}}\r\n    </select>\r\n</div>\r\n",
  "components/common/forms/checkbox": "<div class=\"form-group\" id=\"{{id}}\" data-validation=\"{{validation}}\" {{#if private_id}}data-type=\"{{private_id}}\"{{/if}}>\r\n    <label class=\"form-label\" for=\"{{id}}\">{{label}}\r\n        {{#if required}}\r\n            <span class=\"required\">{{lang 'common.required' }}</span>\r\n        {{/if}}\r\n    </label>\r\n    {{#if checkboxes}}\r\n        {{#each checkboxes}}\r\n            <input type=\"checkbox\" value=\"{{value}}\" name=\"{{name}}\" id=\"{{name}}\" data-label=\"{{label}}\" class=\"form-checkbox\" {{#if checked}}checked{{/if}} {{#if private_id}}data-field-type=\"{{private_id}}\"{{/if}}>\r\n            <label class=\"form-label {{class}}\" for=\"{{name}}\">{{label}}</label>\r\n        {{/each}}\r\n    {{/if}}\r\n</div>\r\n",
  "components/common/forms/text": "<div class=\"form-group {{class_name}}\" id=\"{{id}}\" data-validation=\"{{validation}}\"{{#if private_id}} data-type=\"{{private_id}}\"{{/if}}>\r\n    <label class=\"{{class_name}}\" for=\"{{id}}_input\">{{label}}\r\n        {{#if required}}<span class=\"required\">{{lang 'common.required' }}</span>{{/if}}\r\n    </label>\r\n    <input type=\"text\" id=\"{{id}}_input\" data-label=\"{{label}}\" name=\"{{name}}\" class=\"form-input form-control\"{{#if value}} value=\"{{value}}\"{{/if}}{{#if placeholder}} placeholder=\"{{placeholder}}\"{{/if}}{{#if required}} aria-required=\"true\"{{/if}}{{#if private_id}} data-field-type=\"{{private_id}}\"{{/if}}{{#if maxlength}} maxlength=\"{{maxlength}}\"{{/if}}>\r\n</div>\r\n"
}