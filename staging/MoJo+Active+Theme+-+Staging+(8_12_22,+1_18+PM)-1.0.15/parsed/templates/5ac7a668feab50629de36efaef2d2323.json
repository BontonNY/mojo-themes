{
  "components/products/product-details-table": "<table class=\"table table-striped m-0 product-details-table\" style=\" border-radius: 4px;\r\nbox-shadow: 0px 3px 7px 0 rgba(0, 0, 0, 0.25);\">\r\n    {{#unless (occurrences (toLowerCase theme_settings.pdp-brand-list) (toLowerCase product.brand.name))}}\r\n        {{#if product.brand}}\r\n        <tr>\r\n            <th>Brand</th>\r\n            <td {{#if schema}} itemprop=\"brand\" itemscope itemtype=\"http://schema.org/Brand\" {{/if}}>\r\n                <meta itemprop=\"name\" content=\"{{product.brand.name}}\" />\r\n                {{product.brand.name}}\r\n            </td>\r\n        </tr>\r\n        {{/if}}\r\n    {{/unless}}\r\n    \r\n    {{#each product.custom_fields}}\r\n    <tr data-field-row=\"{{name}}\">\r\n        <th>{{name}}</th>\r\n        <td>{{value}}</td>\r\n    </tr>\r\n    {{/each}}\r\n</table>\r\n<button id=\"table-expansion\" class=\"btn btn-lg btn-outline-primary mt-3 font-weight-bold d-none\">\r\n    <span class=\"mr-2\">EXPAND</span> \r\n    <i class=\"fa fa-plus-circle\"></i>\r\n</button>\r\n"
}