{
  "components/account/orders-list": "<ul class=\"list-unstyled\">\r\n    <li class=\"border-bottom py-2 d-none d-lg-block\">\r\n        <div class=\"row header-row\">\r\n            <div class=\"col-12 col-lg-4 mt-2 mt-lg-0\">\r\n                <strong>Order</strong>\r\n            </div>\r\n            <div class=\"col-12 col-lg-2\">\r\n                <strong>Order Date</strong>\r\n            </div>\r\n            <div class=\"col-12 col-lg-2\">\r\n                <strong>Last Update</strong>\r\n            </div>\r\n            <div class=\"col-12 col-lg-2 mt-2 mt-lg-0\">\r\n                <strong>Status</strong>\r\n            </div>\r\n        </div>\r\n    </li>\r\n    \r\n    {{#each orders}}\r\n    <li class=\"border-bottom py-2\">\r\n        <div class=\"row\">\r\n            <div class=\"col-12 col-lg-4 mt-2 mt-lg-0\">\r\n                <div class=\"text-uppercase font-weight-bold\">\r\n                    <a href=\"{{this.details_url}}\">{{lang 'account.orders.list.order_number' number=this.id}}</a>\r\n                </div>\r\n                <p>{{lang 'account.orders.list.product_details' num_products=this.items.length cost=this.total.formatted}}</p>\r\n            </div>\r\n            <div class=\"col-12 col-lg-4\">\r\n                <div class=\"row\">\r\n                    <div class=\"col-6\">\r\n                        <div class=\"text-uppercase d-lg-none font-weight-bold\">{{lang 'account.orders.order_placed' }}</div>\r\n                        <span>{{this.date}}</span>\r\n                    </div>\r\n                    <div class=\"col-6\">\r\n                        <div class=\"text-uppercase d-lg-none font-weight-bold\">{{lang 'account.orders.last_update' }}</div>\r\n                        <span>{{this.last_update_date}}</span>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n            <div class=\"col-12 col-lg-2 mt-2 mt-lg-0 text-left\">\r\n                <div class=\"text-uppercase font-weight-bold d-lg-none\">\r\n                    Status\r\n                </div>\r\n                <span class=\" py-2 font-weight-normal   {{this.status}}\" style=\"font-size:16px;\">{{this.status}}</span>\r\n                {{#if ../settings.returns_enabled}}\r\n                    {{#if this.return_url}}\r\n                        <a class=\"account-orderStatus-action\" href=\"{{this.return_url}}\">\r\n                            {{lang 'account.orders.return_items' }}\r\n                        </a>\r\n                    {{/if}}\r\n                {{/if}}\r\n            </div>\r\n        </div>\r\n    </li>\r\n    {{/each}}\r\n</ul>\r\n"
}