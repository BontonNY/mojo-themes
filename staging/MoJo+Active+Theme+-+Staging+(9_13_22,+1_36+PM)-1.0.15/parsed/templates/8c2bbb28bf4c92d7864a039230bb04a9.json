{"components/common/subscription-form":"<form class=\"form\" action=\"{{urls.subscribe.action}}\" method=\"post\">\r\n    <fieldset class=\"form-fieldset\">\r\n        <legend class=\"sr-only\">{{lang 'newsletter.subscribe'}}</legend>\r\n        <input type=\"hidden\" name=\"action\" value=\"subscribe\">\r\n        <input type=\"hidden\" name=\"nl_first_name\" value=\"bc\">\r\n        <input type=\"hidden\" name=\"check\" value=\"1\">\r\n        <div class=\"form-field\">\r\n            <label class=\"form-label sr-only\" for=\"nl_email\">{{lang 'common.email_address'}}</label>\r\n            <div class=\"d-flex news-flex\">\r\n                <input class=\"form-input form-control\" id=\"nl_email\" name=\"nl_email\" type=\"email\" value=\"\" placeholder=\"{{lang 'newsletter.email_placeholder'}}\" aria-label=\"Enter {{lang 'newsletter.email_placeholder'}} to subscribe to our newsletter.\">\r\n                <button class=\"nl_form_submit btn btn-primary\" type=\"submit\" value=\"Submit\">{{lang 'newsletter.subscribe_submit'}}</button>\r\n                <span class=\"js_error\"></span>\r\n            </div>\r\n            {{#if settings.show_newsletter_summary }}\r\n                <div class=\"footer-newsletter-summary\">{{settings.newsletter_summary}}</div>\r\n            {{/if}}\r\n        </div>\r\n    </fieldset>\r\n</form>\r\n"}
