{
  "components/products/description": "<ul class=\"product-description-accordian\" data-tab role=\"tablist\">\r\n    <li class=\"acc-item is-active\" role=\"presentation\">\r\n        <a class=\"acc-link\" href=\"#tab-description\" role=\"tab\" tabindex=\"1\" aria-selected=\"true\" >Product Details</a>\r\n        <i class=\"fal fa-minus \"></i>\r\n    </li>\r\n    <li id=\"tab-description\">\r\n        <div class=\"tab-pane py-3\" >\r\n            {{{product.description}}}\r\n \r\n\r\n            <p><strong>Item: </strong> #{{product.sku}}</p>\r\n\r\n            {{#each product.custom_fields}}\r\n                \r\n                {{#and  (if name '==' 'prop65') ( if (toLowerCase value) '==' 'yes') }}\r\n                    <!-- <strong>Warning:</strong> -->\r\n                    <div class=\"alert alert-default d-flex p-0 my-4 prop65Warning\">\r\n                        <div class=\"alertBox-column alertBox-icon\">\r\n                            <icon glyph=\"ic-success\" class=\"icon\" aria-hidden=\"true\"><svg xmlns=\"http://www.w3.org/2000/svg\" width=\"24\" height=\"24\" viewBox=\"0 0 24 24\"><path d=\"M12 2C6.48 2 2 6.48 2 12s4.48 10 10 10 10-4.48 10-10S17.52 2 12 2zm1 15h-2v-2h2v2zm0-4h-2V7h2v6z\"></path></svg></icon>\r\n                        </div>\r\n                        <p class=\"m-0 ml-2\">\r\n                            <span>\r\n                                California Residents - Prop65 Warning. This product can expose you to chemical which are known to the State of California to cause cancer, For more information, go to <a href=\"//www.p65warnings.ca.gov\" target=\"_blank\">www.p65warnings.ca.gov</a>\r\n                            </span>\r\n                        </p>\r\n                    </div>\r\n\r\n                {{/and}}\r\n            {{/each}}\r\n\r\n\r\n             \r\n        </div>\r\n    </li>\r\n\r\n \r\n    {{#if product.warranty}}\r\n        <li class=\"acc-item\" role=\"presentation\">\r\n            <a class=\"acc-link\" href=\"#tab-warranty\" role=\"tab\" tabindex=\"2\" aria-selected=\"true\">{{lang 'products.warranty'}}</a>\r\n            <i class=\"fal fa-plus\"></i>\r\n        </li>\r\n        <li id=\"tab-warranty\" style=\"display:none\">\r\n            <div  class=\"tab-pane py-3\" >\r\n                {{{product.warranty}}}\r\n            </div>\r\n        </li>\r\n    {{/if}}         \r\n\r\n</ul>\r\n"
}