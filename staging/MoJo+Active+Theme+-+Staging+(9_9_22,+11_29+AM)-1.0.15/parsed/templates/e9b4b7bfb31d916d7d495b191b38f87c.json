{
  "components/faceted-search/selected-facets": "<div class=\"facetedSearch-refineFilters sidebarBlock\">\n    <h6 class=\"sidebarBlock-heading font-weight-bold d-none\">\n        {{lang 'search.faceted.selected.title'}}\n    </h6>\n\n    {{#if items}}\n        <ul class=\"inlineList inlineList--labels\">\n            {{#each items}}\n                <li>\n                    <a href=\"{{url}}\"\n                       class=\"facetLabel\"\n                       rel=\"nofollow\"\n                       data-faceted-search-facet\n                    >\n                        {{#if facet '===' 'rating'}}\n                            {{lang 'search.faceted.selected.rating-label' rating=title}}\n                        {{else}}\n                            {{{sanitize title}}}\n                        {{/if}}\n\n                       <i class=\"fal fa-times\"></i>\n                    </a>\n                </li>\n            {{/each}}\n        </ul>\n    {{else}}\n    {{/if}}\n\n    {{#if remove_all_url}}\n        <a href=\"{{remove_all_url}}\" data-faceted-search-facet class=\"btn-clearFilters\">\n            {{lang 'search.faceted.selected.clear-all'}}\n        </a>\n    {{/if}}\n</div>\n"
}