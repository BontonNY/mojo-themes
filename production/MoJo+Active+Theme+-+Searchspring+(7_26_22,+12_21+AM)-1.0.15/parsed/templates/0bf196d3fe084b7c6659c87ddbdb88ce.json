{
  "components/common/wishlist-dropdown": "<!-- <form action=\"{{product.add_to_wishlist_url}}\" class=\"form form-wishlist form-action position-relative\" data-wishlist-add method=\"post\">\n    <button class=\"btn btn-outline-primary w-60 h-60 br-100\" type=\"submit\" value=\"{{lang 'account.wishlists.add_to_default'}}\">\n        <span class=\"d-none\">{{lang 'account.wishlists.wishlist'}}</span><i aria-hidden=\"true\" class=\" fal fa-heart\"></i>\n    </button>\n</form> -->\n\n\n<form action=\"{{product.add_to_wishlist_url}}\" class=\"form form-wishlist form-action\" data-wishlist-add method=\"post\">\n\n    \n\n    <a aria-controls=\"wishlist-dropdown\"\n       aria-expanded=\"false\"\n       class=\"button dropdown-menu-button btn btn-outline-primary w-60 h-60 br-100\"\n       data-dropdown=\"wishlist-dropdown\"\n       href=\"#\"\n    >\n        \n        <span class=\"d-none\">{{lang 'account.wishlists.add_item'}}</span>\n        <i aria-hidden=\"true\" class=\" fal fa-heart\"></i>\n        \n    </a>\n    <ul aria-hidden=\"true\" class=\"dropdown-menu\" data-dropdown-content id=\"wishlist-dropdown\">\n        <li  class=\"label\">\n            <label>Add To Wish List: </label>\n        </li>\n\n        {{#if customer.wishlists}}\n            {{#each customer.wishlists}}\n                <li>\n                    <input class=\"button no-btn {{#if this.num_items '!==' '0'}}button--has-items{{/if}}\"\n                           formaction=\"{{this.add_url}}&product_id={{../product.id}}\"\n                           type=\"submit\"\n                           value=\"{{this.name}}\"\n                    >\n                </li>\n            {{/each}}\n        {{else}}\n        <li>\n            <input class=\"no-btn\" type=\"submit\" value=\"{{lang 'account.wishlists.add_to_default'}}\">\n        </li>\n        {{/if}}\n        <li class=\"create-new\">\n            <a data-wishlist class=\"button\" href=\"/wishlist.php?action=addwishlist&product_id={{product.id}}\">\n                <i class=\"fal fa-plus-circle\"></i>\n                {{lang 'account.wishlists.create_new'}}\n            </a>\n        </li>\n    </ul>\n</form>\n"
}