{
  "components/common/small-paginator": "<nav class=\"pagination-list small-paginator\">\r\n    <ul class=\"pagination m-0\">\r\n        {{#if previous}}\r\n            <li class=\"page-item page-item--previous\">\r\n                <a class=\"page-link nowrap\" href=\"{{previous}}\" data-faceted-search-facet>\r\n                    <!-- <i class=\"icon\" aria-hidden=\"true\">\r\n                        <svg>\r\n                            <use xmlns:xlink=\"http://www.w3.org/1999/xlink\" xlink:href=\"#icon-chevron-left\"></use>\r\n                        </svg>\r\n                    </i>-->\r\n                    {{lang 'common.previous'}}\r\n                </a>\r\n            </li>\r\n        {{/if}}\r\n\r\n        {{#if this.links.length}}\r\n            <li class=\"page-item nowrap d-flex align-items-center justify-content-center\">{{lang 'common.paginator.page_of' current=this.current total=this.links.length}}</li>\r\n        {{else}}\r\n            <li class=\"page-item nowrap d-flex align-items-center justify-content-center\">{{lang 'common.paginator.page_of' current=1 total=1}}</li>\r\n        {{/if}}\r\n\r\n        {{#if next}}\r\n            <li class=\"page-item page-item--next\">\r\n                <a class=\"page-link nowrap\" href=\"{{next}}\" data-faceted-search-facet>\r\n                    {{lang 'common.next'}}\r\n                    <!-- <i class=\"icon\" aria-hidden=\"true\">\r\n                        <svg>\r\n                            <use xmlns:xlink=\"http://www.w3.org/1999/xlink\" xlink:href=\"#icon-chevron-right\"></use>\r\n                        </svg>\r\n                    </i>-->\r\n                </a>\r\n            </li>\r\n        {{/if}}\r\n    </ul>\r\n</nav>\r\n\r\n"
}