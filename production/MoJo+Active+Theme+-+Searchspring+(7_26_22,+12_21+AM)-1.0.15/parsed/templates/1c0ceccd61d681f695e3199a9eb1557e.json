{
  "components/faceted-search/facets/range": "<div class=\"accordion-block\">\n    {{> components/faceted-search/faceted-search-navigation}}\n\n    <div id=\"facetedSearch-content--{{dashcase facet}}\" class=\"accordion-content \">\n        <form id=\"facet-range-form\" class=\"form\" method=\"get\" data-faceted-search-range novalidate>\n            {{#each current_selected_items}}\n                <input type=\"hidden\" name=\"{{param_name}}[]\" value=\"{{param_value}}\"/>\n            {{/each}}\n            <input type=\"hidden\" name=\"search_query\" value=\"{{search_query}}\">\n            {{#if this.sort}}\n                <input type=\"hidden\" name=\"sort\" value=\"{{this.sort}}\">\n            {{/if}}\n\n            <input type=\"hidden\"  name=\"min_price\" value=\"{{min_price}}\">\n            <input type=\"hidden\"  name=\"max_price\" value=\"{{max_price}}\">\n\n            <div class=\"price-range-check\">\n                <label for=\"min_max_50\"> \n                    <input id=\"min_max_50\" type=\"checkbox\" name=\"price-range\" class=\"price-checkbox\"  {{#if min_price '==' 0 }} checked {{/if}}  {{#if max_price '==' 50 }} checked {{/if}} data-min-price=\"0\" data-max-price=\"50\">\n                    Under $50\n                </label>\n                <label for=\"min_max_100\"> \n                    <input id=\"min_max_100\" type=\"checkbox\" name=\"price-range\" class=\"price-checkbox\" {{#if min_price '==' 50 }} checked {{/if}} {{#if max_price '==' 100 }} checked {{/if}} data-min-price=\"50\" data-max-price=\"100\">\n                    $50 - $100\n                </label>\n                <label for=\"min_max_250\"> \n                    <input id=\"min_max_250\" type=\"checkbox\" name=\"price-range\" class=\"price-checkbox\" {{#if min_price '==' 100 }} checked {{/if}} {{#if max_price '==' 250 }} checked {{/if}} data-min-price=\"100\" data-max-price=\"250\">\n                    $100 - $250\n                </label>\n                <label for=\"min_max_500\"> \n                    <input id=\"min_max_500\" type=\"checkbox\" name=\"price-range\" class=\"price-checkbox\" {{#if min_price '==' 250 }} checked {{/if}} {{#if max_price '==' 500 }} checked {{/if}} data-min-price=\"250\" data-max-price=\"500\">\n                    $250 - $500\n                </label>\n                <label for=\"min_max_500_above\"> \n                    <input id=\"min_max_500_above\" type=\"checkbox\" name=\"price-range\" class=\"price-checkbox\" {{#if min_price '==' 500 }} checked {{/if}} {{#if max_price '==' 999999 }} checked {{/if}} data-min-price=\"500\" data-max-price=\"999999\">\n                    $500 & Above\n                </label>\n            </div>\n\n            <label class=\"label\"> Custom Price Range </label>\n\n            <fieldset class=\"form-fieldset\">\n                <div class=\"form-minMaxRow\">\n                    <div class=\"form-field mb-2\">\n                        <input \n                            placeholder=\"{{lang 'search.faceted.range.min-placeholder'}}\"\n                            min=\"1\"\n                            class=\"form-control min-field\"\n                            required\n                            type=\"number\"\n                            value=\"{{min_price}}\"\n                        />\n                    </div>\n\n                    <div class=\"form-field mb-2\">\n                        <input \n                            placeholder=\"{{lang 'search.faceted.range.max-placeholder'}}\"\n                            min=\"1\"\n                            class=\"form-control max-field\"\n                            required\n                            type=\"number\"\n                            value=\"{{max_price}}\"\n                        />\n                    </div>\n\n                    <div class=\"form-field\">\n                        <button class=\"btn btn-primary\" type=\"submit\">\n                            {{lang 'search.faceted.range.update'}}\n                        </button>\n                    </div>\n                </div>\n\n                <div class=\"form-inlineMessage\"></div>\n            </fieldset>\n        </form>\n    </div>\n</div>\n\n\n",
  "components/faceted-search/faceted-search-navigation": "<div class=\"accordion-nav-clear-holder d-flex justify-content-between\">\n    <div class=\"accordion-title\">\n        <span class=\"font-weight-bold\">\n            {{title}}\n            {{#if selected}}\n                <a href=\"{{remove_url}}\" class=\"facetedSearch-clearLink\" data-faceted-search-facet> ({{lang 'search.faceted.clear'}}) </a>\n            {{/if}}\n        </span>\n        <i class=\"fal fa-plus\"></i>\n    </div>\n     \n</div>\n"
}