{
  "components/account/address-list": "{{#unless customer.addresses}}\r\n    {{> components/common/alert-info (lang 'account.addresses.no_addresses')}}\r\n{{/unless}}\r\n\r\n<ul class=\"list-unstyled row my-3\">\r\n    {{#each customer.addresses}}\r\n        <li class=\"col-4 pl-0\">\r\n            <div class=\"card\">\r\n                <div class=\"card-body bg-light text-left\">\r\n                    <div class=\"h6\">{{first_name}} {{last_name}}</div>\r\n                    <ul class=\"list-unstyled\">\r\n                        <li>{{{company}}}</li>\r\n                        <li>{{{address1}}}</li>\r\n                        <li>{{{address2}}}</li>\r\n                        <li>{{{city}}}{{#if state}}, {{state}}{{/if}} {{zip}}</li>\r\n                        <li>{{country}}</li>\r\n                    </ul>\r\n                    {{#if phone}}\r\n                        <dl class=\"address-details\">\r\n                            <dt class=\"address-label\">{{lang 'account.addresses.phone' }}</dt>\r\n                            <dd class=\"address-description\">{{phone}}</dd>\r\n                        </dl>\r\n                    {{/if}}\r\n                    <form action=\"{{delete_url}}\" method=\"post\" data-delete-address=\"{{lang 'forms.address.confirm_delete'}}\">\r\n                        <div class=\"form-actions\">\r\n                            <button type=\"submit\" class=\"btn btn-outline-secondary btn-sm\"> \r\n                                <i class=\"fas fa-trash\"></i>\r\n                                <span class=\"sr-only\">{{lang 'common.delete' }}</span>\r\n                            </button>\r\n                            <a class=\"btn btn-outline-secondary btn-sm\" href=\"{{edit_url}}\">\r\n                                <i class=\"fas fa-pencil\"></i>\r\n                                {{lang 'common.edit' }}\r\n                            </a>\r\n                        </div>\r\n                    </form>\r\n                </div>\r\n            </div>\r\n        </li>\r\n    {{/each}}\r\n    <li class=\"col-4\">\r\n        <a class=\"card\" href=\"{{urls.account.add_address}}\">\r\n            <span class=\"card-body bg-light d-flex justify-content-center align-items-center\">\r\n                <span class=\"address-addNew\">\r\n                    <i class=\"fas fa-plus fa-2x\"></i>\r\n                    <div class=\"h6\">{{lang 'account.addresses.new_address' }}</div>\r\n                </span>\r\n            </span>\r\n        </a>\r\n    </li>\r\n</ul>\r\n",
  "components/common/alert-info": "  \r\n<div class=\"card my-4 col-12 col-lg-6 p-0 \">\r\n    <div class=\"card-body p-0\">\r\n        <i class=\"fas fa-info-circle\"></i> {{{this}}}\r\n    </div>\r\n</div>  \r\n"
}