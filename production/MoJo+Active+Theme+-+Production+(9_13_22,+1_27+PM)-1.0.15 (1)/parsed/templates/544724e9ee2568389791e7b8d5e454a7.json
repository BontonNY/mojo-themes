{"components/account/create-wishlist":"<div class=\"wishlist-add\">\r\n    <div class=\"wishlist-div\">\r\n        {{#if forms.wishlist.id}}\r\n            <h6>{{lang 'account.wishlists.edit'}}</h6>\r\n        {{else}}\r\n            <h6>{{lang 'account.wishlists.new'}}</h6>\r\n            <p>{{lang 'account.wishlists.tag_line'}}</p>\r\n        {{/if}}\r\n        {{#if forms.wishlist.errors}}\r\n            {{> components/common/alert-error forms.wishlist.errors}}\r\n        {{/if}}\r\n    </div>\r\n    <div>\r\n        <div class=\"form-group\">\r\n            <label class=\"form-label\" for=\"wishlistname\">\r\n                {{lang 'forms.wishlist.name'}}\r\n                <span class=\"required\">{{lang 'common.required' }}</span>\r\n            </label>\r\n            <input class=\"form-control\" name=\"wishlistname\" id=\"wishlistname\" value=\"{{forms.wishlist.name}}\">\r\n        </div>\r\n        <div class=\"form-field\">\r\n            <input type=\"checkbox\" class=\"form-checkbox\" name=\"publicwishlist\" id=\"publicwishlist\">\r\n            <label class=\"form-label\" for=\"publicwishlist\">{{lang 'forms.wishlist.public' }}</label>\r\n        </div>\r\n        <div class=\"form-actions\">\r\n            <input type=\"button\" class=\"btn-create-wishlist btn btn-primary\" value=\"{{lang 'account.wishlists.create'}}\">            \r\n        </div>\r\n    </div>\r\n</div>\r\n","components/common/alert-error":"<div class=\"alert alert-danger d-flex align-items-center\">\r\n    <div class=\"alertBox-column alertBox-icon\">\r\n        <icon glyph=\"ic-error\" class=\"icon\" aria-hidden=\"true\"><svg xmlns=\"http://www.w3.org/2000/svg\" width=\"24\" height=\"24\" viewBox=\"0 0 24 24\"><path d=\"M12 2C6.48 2 2 6.48 2 12s4.48 10 10 10 10-4.48 10-10S17.52 2 12 2zm1 15h-2v-2h2v2zm0-4h-2V7h2v6z\"></path></svg></icon>\r\n    </div>\r\n    <p class=\"m-0 ml-2\">\r\n        <span>{{{this}}}</span>\r\n    </p>\r\n</div>\r\n"}
