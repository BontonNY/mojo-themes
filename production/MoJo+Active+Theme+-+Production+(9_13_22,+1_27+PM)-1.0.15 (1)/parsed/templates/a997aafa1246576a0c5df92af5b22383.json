{"components/products/options/set-select":"<div class=\"form-field\" data-product-attribute=\"set-select\">\r\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" for=\"attribute_select_{{id}}\">\r\n        <span>{{ this.display_name }}:\r\n        <span data-option-value>Not Selected</span>\r\n        {{#if required}}\r\n            <span class=\"required\">{{lang 'common.required' }}</span>\r\n        {{/if}}\r\n        </span>\r\n        \r\n        {{#if this.display_name '==' 'Size'}}\r\n            <span class=\"d-none px-3\">|</span>\r\n            {{#comment}}\r\n            <!--\r\n            <a href=\"#\" class=\"size-chart-popup\" id=\"size-chart-popup\">Size Chart</a>\r\n            -->\r\n            {{/comment}}\r\n            <div class='prikid-size-chart-button-placeholder'></div>\r\n        {{/if}}\r\n    </label>\r\n\r\n    <select class=\"form-select form-select--small form-control\" name=\"attribute[{{this.id}}]\" id=\"attribute_select_{{id}}\" {{#if required}}required{{/if}}>\r\n        <option value=\"\">{{lang 'products.choose_options'}}</option>\r\n        {{#each this.values}}\r\n            <option data-value=\"{{this.label}}\" data-product-attribute-value=\"{{id}}\" value=\"{{id}}\" {{#if selected}}selected data-default{{/if}}>{{label}}</option>\r\n        {{/each}}\r\n    </select>\r\n</div>   \r\n \r\n \r\n"}
