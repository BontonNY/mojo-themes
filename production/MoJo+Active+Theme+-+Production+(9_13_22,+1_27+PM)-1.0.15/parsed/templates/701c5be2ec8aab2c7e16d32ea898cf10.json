{"components/category/toolbar":"<div class=\"form-fieldset actionBar-section col-per-row d-none d-md-block mt-2 mt-md-auto mb-md-auto\">\r\n    <legend class=\"sr-only\">Columns Per Rows</legend>\r\n    <div class=\"sort-control-container\">\r\n        <label class=\"form-label sr-only\" for=\"c_rows\">{{lang 'common.number_products_rows.show'}}:</label>\r\n        <span>{{lang 'common.number_products_rows.show'}}: </span>\r\n        <select class=\"form-select form-select-numbers-only\" id=\"c_rows\">\r\n            <option value=\"less\">4</option>\r\n            <option value=\"more\">6</option>\r\n        </select>\r\n    </div>\r\n</div>\r\n    \r\n<div class=\"form-fieldset actionBar-section  product-per-page mt-2 mt-md-auto mb-md-auto\">\r\n    <legend class=\"sr-only\">Products Per Page</legend>\r\n    <div class=\"sort-control-container\">\r\n        <label class=\"form-label sr-only\" for=\"limit\">{{lang 'common.number_products.show'}}:</label>\r\n        <span class=\"d-none d-md-block\">{{lang 'common.number_products.show'}}: </span>\r\n        <select class=\"form-select form-select-numbers-only\" name=\"limit\" id=\"limit\" onchange=\"location='?limit='+this.value\">\r\n            <option value=\"8\"  {{#if '8' '==' theme_settings.categorypage_products_per_page }}selected{{/if}}>8 </option>\r\n            <option value=\"12\" {{#if '12' '==' theme_settings.categorypage_products_per_page }}selected{{/if}}>12 </option>\r\n            <option value=\"24\" {{#if '24' '==' theme_settings.categorypage_products_per_page }}selected{{/if}}>24</option>\r\n            <option value=\"48\" {{#if '48' '==' theme_settings.categorypage_products_per_page }}selected{{/if}}>48</option>\r\n        </select>\r\n    </div>\r\n</div>\r\n"}
