{"components/common/forms/multiline":"<div class=\"form-group\" id=\"{{id}}\" data-validation=\"{{validation}}\">\r\n    <label class=\"form-label\" for=\"{{id}}_input\">{{label}}\r\n        {{#if required}}\r\n            <span class=\"required\">{{lang 'common.required' }}</span>\r\n        {{/if}}\r\n    </label>\r\n    <textarea name=\"{{name}}\" id=\"{{id}}_input\" data-label=\"{{label}}\" rows=\"{{rows}}\" aria-required=\"{{required}}\" class=\"form-control\" {{#if private_id}}data-field-type=\"{{private_id}}\"{{/if}}>{{value}}</textarea>\r\n</div>\r\n"}
