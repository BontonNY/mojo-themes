{
  "components/account/navigation": "<nav class=\"bg-light border border-gray rounded navBar--account mb-5\">\r\n    <ul class=\"nav justify-content-center underline-links-on-hover\">\r\n        {{#if account_page '===' 'settings'}}\r\n            <li class=\"nav-item my-2 is-active \"><span class=\"nav-link\">{{lang 'account.nav.settings'}}</span></li>\r\n        {{else}}\r\n            <li class=\"navBar-item my-2\">\r\n                <a class=\"nav-link\" href=\"{{urls.account.details}}\">{{lang 'account.nav.settings'}}</a>\r\n            </li>\r\n        {{/if}}       \r\n\r\n        \r\n        {{#if account_page '===' 'orders'}}\r\n            {{#if page_type '===' 'account_order'}}\r\n                <li class=\"nav-item my-2\">\r\n                    <a class=\"nav-link active\" href=\"{{urls.account.orders.all}}\">{{lang 'account.nav.orders'}}</a>\r\n                    <!-- <a class=\"nav-link active\" href=\"{{#if settings.store_hash '===' 'fymac37tn1'}}https://orders.bonton.com/{{else if settings.store_hash '===' '1c5hrsw45g'}}https://returns.bonton.com/{{else}}urls.account.orders.all{{/if}}\" target=\"{{#if settings.store_hash '===' '9ywlhf4tex'}}_self{{else}}_blank{{/if}}\">{{lang 'account.nav.orders'}}</a> -->\r\n                </li>\r\n            {{else}}\r\n                <li class=\"nav-item my-2\"><span class=\"nav-link active\">{{lang 'account.nav.orders'}}</span></li>\r\n            {{/if}}\r\n        {{else}}\r\n            <li class=\"nav-item my-2\">\r\n                <a class=\"nav-link\" href=\"{{urls.account.orders.all}}\">{{lang 'account.nav.orders'}}</a>\r\n                <!-- <a class=\"nav-link\" href=\"{{#if settings.store_hash '===' 'fymac37tn1'}}https://orders.bonton.com/{{else if settings.store_hash '===' '1c5hrsw45g'}}https://returns.bonton.com/{{else}}urls.account.orders.all{{/if}}\" target=\"{{#if settings.store_hash '===' '9ywlhf4tex'}}_self{{else}}_blank{{/if}}\">{{lang 'account.nav.orders'}}</a> -->\r\n            </li>\r\n        {{/if}}\r\n        {{#comment}}\r\n        <!--\r\n        {{#if settings.returns_enabled}}\r\n            {{#if account_page '===' 'returns'}}\r\n                <li class=\"nav-item my-2\"><span class=\"nav-link\">{{lang 'account.nav.returns'}}</span></li>\r\n            {{else}}\r\n                <li class=\"nav-item my-2\">\r\n                    <a class=\"nav-link active\" href=\"{{urls.account.returns}}\">{{lang 'account.nav.returns'}}</a>\r\n                </li>\r\n            {{/if}}\r\n        {{/if}}\r\n        {{#if account_page '===' 'returns'}}\r\n            <li class=\"nav-item my-2 is-active\"><span class=\"nav-link\">{{lang 'account.nav.returns'}}</span></li>\r\n        {{else}}\r\n            <li class=\"nav-item my-2\">\r\n                <a class=\"nav-link\" href=\"/account/returns/\">{{lang 'account.nav.returns'}}</a>\r\n            </li>\r\n        {{/if}}\r\n        {{#if account_page '===' 'messages'}}\r\n            <li class=\"nav-item my-2\"><span class=\"nav-link\">{{lang 'account.nav.messages' num_new_messages=customer.num_new_messages}}</span></li>\r\n        {{else}}\r\n            <li class=\"navBar-item my-2\">\r\n                <a class=\"nav-link active\" href=\"{{urls.account.inbox}}\">{{lang 'account.nav.messages' num_new_messages=customer.num_new_messages}}</a>\r\n            </li>\r\n        {{/if}}\r\n        -->\r\n        {{/comment}}                \r\n        {{#if account_page '===' 'addresses'}}\r\n            <li class=\"nav-item my-2\"><span class=\"nav-link\">{{lang 'account.nav.addresses'}}</span></li>\r\n        {{else}}\r\n            <li class=\"navBar-item my-2\">\r\n                <a class=\"nav-link active\" href=\"{{urls.account.addresses}}\">{{lang 'account.nav.addresses'}}</a>\r\n            </li>\r\n        {{/if}}\r\n        {{#if settings.show_payment_methods}}\r\n            {{#if account_page '===' 'payment_methods'}}\r\n                <li class=\"nav-item my-2 is-active\"><span class=\"nav-link\">{{lang 'account.nav.payment_methods'}}</span></li>\r\n            {{else}}\r\n                <li class=\"navBar-item my-2\">\r\n                    <a class=\"nav-link\" href=\"{{urls.account.payment_methods.all}}\">{{lang 'account.nav.payment_methods'}}</a>\r\n                </li>\r\n            {{/if}}\r\n        {{/if}}\r\n        {{#if settings.show_wishlist}}\r\n            {{#if account_page '===' 'wishlists'}}\r\n                <li class=\"nav-item my-2 is-active\">\r\n                    <a href=\"{{urls.account.wishlists.all}}\">\r\n                        <span class=\"nav-link\">{{lang 'account.nav.wishlists' num_wishlists=customer.num_wishlists}}</span>\r\n                    </a>\r\n                </li>\r\n            {{else}}            \r\n                {{#if account_page '===' 'returns'}}\r\n                    {{assignVar \"count\" 0}}\r\n                    {{#each customer.wishlists}}\r\n                        <span style=\"display: none !important;\">{{incrementVar \"count\"}}</span>\r\n                    {{/each}}\r\n                    <li class=\"navBar-item my-2\">\r\n                        <a class=\"nav-link\" href=\"{{urls.account.wishlists.all}}\">{{lang 'account.nav.wishlists' num_wishlists=(getVar \"count\")}}</a>\r\n                    </li>                \r\n                {{else}}\r\n                    <li class=\"navBar-item my-2\">\r\n                        <a class=\"nav-link\" href=\"{{urls.account.wishlists.all}}\">{{lang 'account.nav.wishlists' num_wishlists=customer.num_wishlists}}</a>\r\n                    </li>\r\n                {{/if}}\r\n            {{/if}}\r\n        {{/if}}\r\n        {{#if account_page '===' 'recent_items'}}\r\n            <li class=\"nav-item my-2 is-active\"><span class=\"nav-link\">{{lang 'account.nav.recently_viewed'}}</span></li>\r\n        {{else}}\r\n            <li class=\"navBar-item my-2\">\r\n                <a class=\"nav-link\" href=\"{{urls.account.recent_items}}\">{{lang 'account.nav.recently_viewed'}}</a>\r\n            </li>\r\n        {{/if}}\r\n\r\n\r\n\r\n        {{#if customer}}\r\n            <li class=\"navBar-item my-2\">\r\n                <a class=\"nav-link\" href=\"{{urls.auth.logout}}\" aria-label=\"{{lang 'common.logout'}}\">{{lang 'common.logout'}}</a>\r\n            </li>\r\n        {{/if}}              \r\n    </ul>\r\n</nav>\r\n"
}