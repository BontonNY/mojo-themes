{
  "components/account/messages-list": "<div class=\"account-list row flex-column\">\n    {{#each messages}}\n    <div class=\"account-divstItem mb-2\">\n        <div class=\"card border text-left p-3\">\n            <div class=\"account-message\">\n                <div class=\"account-orderStatus\">\n                    <span>{{date}}</span>\n                </div>\n\n                <h6 class=\"account-product-title {{#unless unread}}is-read{{/unless}}\">\n                    {{subject}}\n                </h6>\n                <div class=\"{{#unless unread}}is-read{{/unless}}\">\n                    <span class=\"d-block font-italic\">\n                        {{#if is_reply}}\n                        {{lang 'account.messages.merchant_said' store_name=../../settings.store_name}}\n                        {{else}}\n                        {{lang 'account.messages.customer_said'}}\n                        {{/if}}\n                    </span>\n                    <strong style=\"white-space: pre-wrap;\">{{message}}</strong>\n                </div>\n            </div>\n        </div>\n    </div>\n    {{/each}}\n</div>\n"
}