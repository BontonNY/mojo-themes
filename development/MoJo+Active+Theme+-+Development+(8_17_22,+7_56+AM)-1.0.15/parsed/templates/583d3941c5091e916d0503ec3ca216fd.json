{
  "components/products/description-tabs": "<ul class=\"tabs\" data-tab>\r\n    <li class=\"tab is-active\">\r\n        <a class=\"tab-title\" href=\"#tab-description\">{{lang 'products.description'}}</a>\r\n    </li>\r\n    {{#if product.warranty}}\r\n        <li class=\"tab\">\r\n            <a class=\"tab-title\" href=\"#tab-warranty\">{{lang 'products.warranty'}}</a>\r\n        </li>\r\n    {{/if}}\r\n    {{#all product.custom_fields theme_settings.show_custom_fields_tabs}}\r\n        <li class=\"tab\">\r\n            <a class=\"tab-title\" href=\"#tab-{{dashcase (lowercase (sanitize theme_settings.pdp-custom-fields-tab-label))}}\">{{sanitize theme_settings.pdp-custom-fields-tab-label}}</a>\r\n        </li>\r\n    {{/all}}\r\n    {{#all settings.show_product_reviews theme_settings.show_product_reviews_tabs product.reviews.total}}\r\n        <li class=\"tab\">\r\n            <a class=\"tab-title productView-reviewTabLink\" href=\"#tab-reviews\">{{lang 'products.reviews.header' total=product.reviews.total}}</a>\r\n        </li>\r\n    {{/all}}\r\n</ul>\r\n<div class=\"tabs-contents\">\r\n    <div class=\"tab-content is-active\" id=\"tab-description\">\r\n        {{{product.description}}}\r\n \r\n        <p><strong>Item: </strong> #{{product.sku}}</p>\r\n        \r\n        {{#each product.custom_fields}}                \r\n            {{#and  (if name '==' 'prop65') ( if (toLowerCase value) '==' 'yes') }}\r\n                <!-- <strong>Warning:</strong> -->\r\n                <div class=\"alert alert-default d-flex p-0 my-4 prop65Warning\">\r\n                    <div class=\"alertBox-column alertBox-icon\">\r\n                        <icon glyph=\"ic-success\" class=\"icon\" aria-hidden=\"true\"><svg xmlns=\"http://www.w3.org/2000/svg\" width=\"24\" height=\"24\" viewBox=\"0 0 24 24\"><path d=\"M12 2C6.48 2 2 6.48 2 12s4.48 10 10 10 10-4.48 10-10S17.52 2 12 2zm1 15h-2v-2h2v2zm0-4h-2V7h2v6z\"></path></svg></icon>\r\n                    </div>\r\n                    <p class=\"m-0 ml-2\">\r\n                        <span>\r\n                            California Residents - Prop65 Warning. This product can expose you to chemical which are known to the State of California to cause cancer, For more information, go to <a href=\"//www.p65warnings.ca.gov\" target=\"_blank\">www.p65warnings.ca.gov</a>\r\n                        </span>\r\n                    </p>\r\n                </div>\r\n            {{/and}}\r\n        {{/each}}\r\n\r\n    </div>\r\n   {{#if product.warranty}}\r\n       <div class=\"tab-content\" id=\"tab-warranty\">\r\n           {{{product.warranty}}}  \r\n       </div>\r\n   {{/if}}\r\n   {{#all product.custom_fields theme_settings.show_custom_fields_tabs}}\r\n      <div class=\"tab-content\" id=\"tab-{{dashcase (lowercase (sanitize theme_settings.pdp-custom-fields-tab-label))}}\">\r\n         <dl class=\"productView-info\">\r\n            {{#each product.custom_fields}}\r\n               <dt class=\"productView-info-name\">{{name}}:</dt>\r\n               <dd class=\"productView-info-value\">{{{value}}}</dd>\r\n            {{/each}}\r\n         </dl>\r\n      </div>\r\n   {{/all}}\r\n   {{#all settings.show_product_reviews theme_settings.show_product_reviews_tabs}}\r\n       <div class=\"tab-content\" id=\"tab-reviews\">\r\n           {{> components/products/reviews reviews=product.reviews product=product urls=urls}}\r\n       </div>\r\n   {{/all}}\r\n</div>\r\n",
  "components/products/reviews": "<section class=\"productReviews\" id=\"product-reviews\" data-product-reviews aria-labelledby=\"rating-block\">\r\n    <h2 class=\"h2 font-weight-normal text-center pt-2 mb-2\">Reviews</h2>\r\n    <div class=\"row mb-2 pb-0 text-center justify-content-center\">\r\n        <div id=\"rating-block\" class=\"col-md-4 col-xl-2\" tabindex=\"0\">\r\n            <p id=\"averageRating\" class=\"font-weight-normal mb-0\" style=\"font-size: 48px; height: 60px;\">{{product.rating}}<span class=\"sr-only\">Stars</span></p>\r\n            <p id=\"ratingStars\" class=\"m-0\" tabindex=\"-1\">{{> components/products/ratings rating=product.rating}}</p>\r\n            <p id=\"NumberInRating\">\r\n                Based on <span>{{reviews.total}}</span> Reviews\r\n            </p>\r\n        </div>\r\n        <div class=\"col-md-4 col-xl-2\">\r\n            <img class=\"lazyload m-auto\" data-sizes=\"auto\" src=\"{{cdn 'img/loading.svg'}}\">\r\n            <div id=\"Review-Graph\" class=\"d-none px-4 px-md-0\">\r\n                <span class=\"d-flex mb-0 align-items-center\">\r\n                    <div class=\"\">{{> components/products/ratings rating=5}}</div>\r\n                    <div class=\"d-flex flex-grow-1 mx-2 pt-2\">\r\n                        <span class=\"line front bg-primary\"></span>\r\n                        <span class=\"line back bg-light\" style=\"width:100%\"></span>\r\n                    </div>\r\n                    <div><span class=\"graph count\">()</span></div>\r\n                </span>\r\n                <span class=\"d-flex mb-0 align-items-center\">\r\n                    <div class=\"\">{{> components/products/ratings rating=4}}</div>\r\n                    <div class=\"d-flex flex-grow-1 mx-2 pt-2\">\r\n                        <span class=\"line front bg-primary\"></span>\r\n                        <span class=\"line back bg-light\" style=\"width:100%\"></span>\r\n                    </div>\r\n                    <div><span class=\"graph count\">()</span></div>\r\n                </span>\r\n                <span class=\"d-flex mb-0 align-items-center\">\r\n                    <div class=\"\">{{> components/products/ratings rating=3}}</div>\r\n                    <div class=\"d-flex flex-grow-1 mx-2 pt-2\">\r\n                        <span class=\"line front bg-primary\"></span>\r\n                        <span class=\"line back bg-light\" style=\"width:100%\"></span>\r\n                    </div>\r\n                    <div><span class=\"graph count\">()</span></div>\r\n                </span>\r\n                <span class=\"d-flex mb-0 align-items-center\">\r\n                    <div class=\"\">{{> components/products/ratings rating=2}}</div>\r\n                    <div class=\"d-flex flex-grow-1 mx-2 pt-2\">\r\n                        <span class=\"line front bg-primary\"></span>\r\n                        <span class=\"line back bg-light\" style=\"width:100%\"></span>\r\n                    </div>\r\n                    <div><span class=\"graph count\">()</span></div>\r\n                </span>\r\n                <span class=\"d-flex mb-0 align-items-center\">\r\n                    <div class=\"\">{{> components/products/ratings rating=1}}</div>\r\n                    <div class=\"d-flex flex-grow-1 mx-2 pt-2\">\r\n                        <span class=\"line front bg-primary\"></span>\r\n                        <span class=\"line back bg-light\" style=\"width:100%\"></span>\r\n                    </div>\r\n                    <div><span class=\"graph count\">()</span></div>\r\n                </span>\r\n            </div>\r\n        </div>\r\n        <div class=\"col-md-4 col-xl-2 d-flex mt-2 mt-md-0\">\r\n            <a class=\"btn btn-lg btn-outline-primary write-review m-auto font-weight-bold\" href=\"{{product.url}}{{#if is_ajax}}#write_review{{/if}}\" {{#unless is_ajax }}data-toggle=\"modal\" data-target=\"#modal-review-form\" {{/unless}} style=\"height: 52px;\">\r\n                <span>{{lang 'products.reviews.new'}}</span> \r\n            </a>\r\n        </div>\r\n    </div>\r\n    {{#comment}}\r\n    <!-- <h4 class=\"toggle-title\">\r\n        {{lang 'products.reviews.header' total=reviews.total}}\r\n        {{#if theme_settings.show_product_reviews_tabs '!==' true}}\r\n            <a class=\"toggleLink is-open\" data-collapsible href=\"#productReviews-content\">\r\n                <span class=\"toggleLink-text toggleLink-text--on\">\r\n                    {{lang 'products.reviews.hide'}}\r\n                </span>\r\n                <span class=\"toggleLink-text toggleLink-text--off >\r\n                    {{lang 'products.reviews.show'}}\r\n                </span>\r\n            </a>\r\n        {{/if}}\r\n    </h4> -->\r\n    {{/comment}}\r\n    <div class=\"toggle-content is-open border-top\" id=\"productReviews-content\" aria-hidden=\"false\">\r\n        {{#each reviews.list}}\r\n            <article class=\"review-article\" itemprop=\"review\" itemscope itemtype=\"http://schema.org/Review\">\r\n                <div class=\"row mx-5 mt-4\">\r\n                    <div class=\"col-lg-2\">\r\n                        <span class=\"productReview-rating\" itemprop=\"reviewRating\" itemscope itemtype=\"http://schema.org/Rating\">\r\n                            {{> components/products/ratings rating=rating}}\r\n                            <span class=\"productReview-ratingNumber\" itemprop=\"ratingValue\">{{ rating }}</span>\r\n                            <meta itemprop=\"ratingValue\" content=\"{{rating}}\">\r\n                                <meta itemprop=\"bestValue\" content=\"5\">\r\n                        </span>\r\n\r\n                        {{#if name}}\r\n                        <p class=\"h4 font-weight-bold mt-2 mb-4\">{{name}}</p>\r\n                        <div itemprop=\"author\" itemtype=\"https://schema.org/Person\" itemscope>\r\n                            <meta itemprop=\"name\" content=\"{{name}}\">\r\n                        </div>\r\n                        {{else}}\r\n                        <p class=\"productReview-author\">\r\n                            {{lang 'products.reviews.post_on_by' date=date name=(lang 'products.reviews.anonymous_poster')}}\r\n                            <div itemprop=\"author\" itemtype=\"https://schema.org/Person\" itemscope>\r\n                                <meta itemprop=\"name\" content=\"{{lang 'products.reviews.anonymous_poster'}}\">\r\n                            </div>\r\n                        </p>\r\n                        {{/if}}\r\n                        <p class=\"font-weight-bold\">\r\n                            {{date}}\r\n                        </p>\r\n                    </div>\r\n                    <div class=\"col-lg-10\">\r\n                        <h2 class=\"h4\" itemprop=\"name\" class=\"productReview-title\">{{{ title }}}</h2>\r\n                        <p itemprop=\"reviewBody\" class=\"productReview-body\">{{nl2br text}}</p>\r\n                    </div>\r\n                </div>\r\n            </article>\r\n        {{/each}}\r\n        <div class=\"col text-center review-pagination\">\r\n            {{> components/common/paginator pagination.reviews}}\r\n            <button id=\"review-expansion\" class=\"btn btn-lg btn-outline-primary my-3 font-weight-bold d-none\">\r\n                <span class=\"mr-2\">MORE REVIEWS</span> \r\n                <i class=\"fa fa-plus-circle\"></i>\r\n            </button>\r\n        </div>\r\n    </div>\r\n</section>\r\n",
  "components/common/paginator": "<nav class=\"Page navigation my-4 d-flex justify-content-between\" aria-label=\"Pagination {{label}}\">\r\n    <ul class=\"pagination justify-content-center mr-1 previous\">\r\n        {{#if previous}}\r\n            <li class=\"page-item page-item--previous\">\r\n                <a class=\"page-link text-dark nowrap\" href=\"{{previous}}\" {{#unless reload}}data-faceted-search-facet{{/unless}} aria-label=\"Previous Page\">\r\n                    <i class=\"fal fa-angle-left\"></i>\r\n                    <span>{{lang 'common.previous'}}</span>\r\n                </a>\r\n            </li>\r\n        {{/if}}\r\n    </ul>\r\n    \r\n    <ul class=\"pagination justify-content-center mr-1 middle\">\r\n        {{#each links}}\r\n            {{#if this.number '==' ../current}}\r\n                <li class=\"page-item\">\r\n                    <a class=\"page-link text-primary active\" href=\"{{this.url}}\" {{#unless reload}}data-faceted-search-facet{{/unless}} aria-label=\"Go To Page {{this.number}}\">{{this.number}}</a>\r\n                </li>\r\n            {{else}}\r\n                <li class=\"page-item\">\r\n                    <a class=\"page-link text-dark\" href=\"{{this.url}}\" {{#unless reload}}data-faceted-search-facet{{/unless}} aria-label=\"Go To Page {{this.number}}\">{{this.number}}</a>\r\n                </li>\r\n            {{/if}}\r\n        {{/each}}\r\n    </ul>\r\n\r\n\r\n    <ul class=\"pagination justify-content-end mr-1 next\">\r\n        {{#if next}}\r\n            <li class=\"page-item page-item--next\">\r\n                <a class=\"page-link text-dark nowrap\" href=\"{{next}}\" {{#unless reload}}data-faceted-search-facet{{/unless}}>\r\n                    <span>{{lang 'common.next'}}</span>        \r\n                    <i class=\"fal fa-angle-right\"></i>\r\n                </a>\r\n            </li>\r\n        {{/if}}\r\n    </ul>\r\n</nav>\r\n",
  "components/products/ratings": "{{#for 1 5}}\r\n    {{#if ../this.rating '>=' $index}}    \r\n    <span class=\"icon icon--ratingFull\" tabindex=\"-1\" focusable=\"false\">\r\n        <i class=\"fa fa-star\"></i>\r\n    </span>\r\n    {{else}}\r\n    <span class=\"icon icon--ratingEmpty\" tabindex=\"-1\" focusable=\"false\">\r\n        <i class=\"fal fa-star\"></i>\r\n    </span>\r\n    {{/if}}\r\n{{/for}}\r\n"
}