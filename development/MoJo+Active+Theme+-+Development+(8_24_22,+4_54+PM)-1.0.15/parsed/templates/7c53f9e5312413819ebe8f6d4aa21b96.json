{
  "components/common/navigation": "<nav class=\"nav-secondary\" aria-label=\"Secondary Menu Tools\">\r\n    {{> components/common/currency-selector}}\r\n\r\n    <ul class=\"nav align-items-center floating-menu-nav justify-content-center justify-content-xxl-end flex-nowrap\">\r\n        {{#comment}}\r\n        <!-- {{#if settings.gift_certificates_enabled}}\r\n        <li class=\"nav-item\">\r\n            <a class=\"nav-link\" href=\"{{urls.gift_certificate.purchase}}\">{{lang 'common.gift_cert'}}</a>\r\n        </li>\r\n        {{/if}} -->\r\n        {{/comment}}\r\n        <li class=\"nav-item position-relative nav-item-form nav-search\">\r\n           <div id=\"ss-search-bar-desktop\">\r\n                <div>\r\n                    {{> components/common/search-box}}\r\n                </div>\r\n           </div>\r\n            <!-- <a class=\"nav-link nav-link--quickSearch\" href=\"#\" data-search=\"quickSearch\" aria-controls=\"quickSearch\" aria-expanded=\"false\"><i class=\"fas fa-search mr-2 text-muted\"></i>{{lang 'common.search'}}</a> -->\r\n        </li>\r\n        {{#if customer.store_credit.value '>' 0}}\r\n        <li class=\"nav-item\">\r\n            <a class=\"nav-link nav-link--storeCredit\" data-dropdown=\"storeCredit-dropdown\" data-options=\"align:bottom\" href=\"{{urls.cart}}\">\r\n                <span class=\"nav-link-divider\">{{lang 'common.store_credit_overview' credit=customer.store_credit.formatted}}</span>\r\n            </a>\r\n            <div class=\"dropdown-menu\" id=\"storeCredit-dropdown\" data-dropdown-content aria-hidden=\"true\">\r\n                {{{lang 'common.store_credit' store_credit=customer.store_credit.formatted}}}\r\n            </div>\r\n        </li>\r\n        {{/if}}\r\n        {{#if theme_settings.social_icon_placement_top}}\r\n        <li class=\"nav-item nav-item--social\">\r\n            {{> components/common/social-links}}\r\n        </li>\r\n        <li class=\"nav-item nav-item--divider\">|</li>\r\n        {{/if}}\r\n        <li id=\"account-hover\" class=\"nav-item position-relative\">\r\n            {{#if customer}}\r\n                <a class=\"nav-link\" href=\"{{urls.account.index}}\" title=\"{{lang 'common.account'}}\"><i class=\"far fa-user\"></i></a>\r\n            \t\t{{> components/common/account-menu}}\r\n            {{else}}        \r\n                <a class=\"nav-link\" href=\"{{urls.auth.login}}\" title=\"{{lang 'common.account'}}\"><i class=\"far fa-user\"></i></a>\r\n            {{/if}}\r\n        </li>\r\n        <li  class=\"nav-item position-relative\">\r\n            <a class=\"navbar-button--addon \" href=\"{{urls.account.wishlists.all}}\">\r\n                <i class=\"far fa-heart\"></i>\r\n            </a>\r\n        </li>\r\n        <li class=\"nav-item nav-item--cart\">\r\n            \r\n            <a class=\"nav-link\" href=\"{{urls.cart}}\" title=\"{{lang 'common.cart'}}\"  {{#unless theme_settings.hide_minicart}} data-cart-preview data-dropdown=\"cart-preview-dropdown\" data-options=\"align:right\"{{/unless}}>\r\n                <span class=\"nav-item-cartLabel\">\r\n                    <i class=\"far fa-shopping-bag text-secondary\"></i>                    \r\n                </span>\r\n                <div class=\"cart-count\">\r\n                    <span class=\"cart-quantity\"></span>\r\n                </div>                \r\n                \r\n            </a>\r\n            {{#unless theme_settings.hide_minicart }}        \r\n                <div class=\"dropdown-menu shadow fadeInRight animated\" id=\"cart-preview-dropdown\" data-dropdown-content aria-hidden=\"true\"></div>\r\n            {{/unless}}\r\n        </li>\r\n    </ul>\r\n</nav>\r\n\r\n",
  "components/common/social-links": "{{#if social_media}}\r\n<div class=\"socialLinks socialLinks--alt\">\r\n    {{#each social_media}}\r\n    <div class=\"socialLinks-item\">\r\n        <a class=\"icon icon--{{display_name}}\" href=\"{{url}}\" target=\"_blank\">\r\n            <i class=\"fab fa-{{display_name}}\"></i>\r\n        </a>\r\n    </div>\r\n    {{/each}}\r\n</div>\r\n{{/if}}\r\n",
  "components/common/account-menu": "<div id=\"account-dropdown\" class=\"dropdown-menu\" data-dropdown-content role=\"document\" tabindex=\"0\" aria-hidden=\"true\">\r\n\t<ul>\r\n\t\t<li class=\"myaccount-link\">\r\n\t\t\t<a href=\"/account.php?action=account_details\"><i class=\"fa-user far\"></i><span>My Account</span></a>\r\n\t\t</li>\r\n\t\t<li class=\"order-link\">\r\n\t\t\t\t<a href=\"/account.php?action=order_status\"><i class=\"fa-receipt far\"></i><span>Orders</span></a>\r\n\t\t\t\t<!-- <a href=\"{{#if settings.store_hash '===' 'fymac37tn1'}}https://orders.bonton.com/{{else if settings.store_hash '===' '1c5hrsw45g'}}https://returns.bonton.com/{{else}}/account.php?action=order_status{{/if}}\" target=\"{{#if settings.store_hash '===' '9ywlhf4tex'}}_self{{else}}_blank{{/if}}\"><i class=\"fa-receipt far\"></i><span>Orders</span></a> -->\r\n\t\t</li>\r\n\t\t<li>\r\n\t\t\t\t<a href=\"/account.php?action=address_book\"><span>Addresses</span></a>\r\n\t\t</li>\r\n\t\t<li class=\"wishlist-link\">\r\n\t\t\t\t<a href=\"{{urls.account.wishlists.all}}\"><i class=\"fa-heart far\"></i><span>Wish Lists</span></a>\r\n\t\t</li>\r\n\t\t<li class=\"recent-link\">\r\n\t\t\t\t<a href=\"/account.php?action=recent_items\"><i class=\"fa-eye far\"></i><span>Recently Viewed</span></a>\r\n\t\t</li>\r\n\t\t<li>\r\n\t\t\t\t<a href=\"/account.php?action=account_details\"><span>Account Settings</span></a>\r\n\t\t</li>\t\t\r\n\t\t<li class=\"logout-link\">\r\n\t\t\t<a href=\"/login.php?action=logout\"><i class=\"fa-sign-out far\"></i><span>Log Out</span></a>\r\n\t\t</li>\r\n\t</ul>\r\n</div>\r\n",
  "components/common/currency-selector": "{{#if currency_selector.currencies.length '>' 1}}\r\n<ul class=\"navUser-section\">\r\n    <li class=\"navUser-item\">\r\n        <a class=\"navUser-action navUser-action--currencySelector has-dropdown\" href=\"#\" data-dropdown=\"currencySelection\" aria-controls=\"currencySelection\" aria-expanded=\"false\">{{lang 'common.currency' code=currency_selector.active_currency_code}} <i class=\"icon\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-down\" /></svg></i></a>\r\n        <ul class=\"dropdown-menu\" id=\"currencySelection\" data-dropdown-content aria-hidden=\"true\" tabindex=\"-1\">\r\n            {{#each currency_selector.currencies}}\r\n            <li class=\"dropdown-menu-item\">\r\n                <a href=\"{{{switch_url}}}\">\r\n                    {{#if is_active}}\r\n                    <strong>{{name}}</strong>\r\n                    {{else}}\r\n                    {{name}}\r\n                    {{/if}}\r\n                </a>\r\n            </li>\r\n            {{/each}}\r\n        </ul>\r\n    </li>\r\n</ul>\r\n{{/if}}\r\n",
  "components/common/search-box": "{{!--\r\n<form class=\"form\" action=\"{{urls.search}}\">\r\n    <fieldset class=\"d-flex px-0 px-xl-0\">\r\n        <legend class=\"sr-only\">Site Search</legend>\r\n        <div class=\"d-flex w-100 search-box-wrapper\">\r\n            <label class=\"sr-only\" for=\"search_query\">{{lang \"search.quick_search.input_label\"}}</label>\r\n            <input class=\"form-control px-4 pr-6 small\" data-search-quick name=\"search_query\" placeholder=\"{{lang 'search.quick_search.input_placeholder'}}\" autocomplete=\"off\">\r\n            <button class=\"d-none btnIconSearch\"><i class=\"fa fa-search\"></i></button>\r\n            <button class=\"search-btn ml-2 px-3 px-lg-2\" type=\"submit\"><i class=\"far fa-search d-inline mr-lg-2\"></i></button>\r\n        </div>\r\n    </fieldset>\r\n    <div class=\"dropdown dropdown--quickSearch quickSearchResults\" aria-hidden=\"true\" tabindex=\"-1\" data-prevent-quick-search-close>\r\n        {{> components/common/quick-search}}\r\n    </div>\r\n</form>\r\n--}}\r\n\r\n<form class=\"form\" method=\"get\" action=\"/shop/\">\r\n    <fieldset class=\"d-flex px-0 px-xl-0\">\r\n        <legend class=\"sr-only\">Site Search</legend>\r\n        <div class=\"d-flex w-100 search-box-wrapper\">\r\n            <label class=\"sr-only\" for=\"search_query\">{{lang \"search.quick_search.input_label\"}}</label>\r\n            <input class=\"searchspring-ac form-control px-4 pr-6 small\" name=\"search_query\" placeholder=\"{{lang 'search.quick_search.input_placeholder'}}\" autocomplete=\"off\">\r\n            <button class=\"d-none btnIconSearch\"><i class=\"fa fa-search\"></i></button>\r\n            <button class=\"search-btn ml-2 px-3 px-lg-2\" type=\"submit\"><i class=\"far fa-search d-inline mr-lg-2\"></i></button>\r\n        </div>\r\n    </fieldset>\r\n</form>\r\n",
